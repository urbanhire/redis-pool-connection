{
  "name": "redis-pool-connection",
  "version": "1.4.0",
  "description": "Re-use a single or pool of redis connections across several modules/files in your app.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/standard && ./node_modules/.bin/istanbul cover ./node_modules/tape/bin/tape ./test/*.test.js"
  },
  "keywords": [
    "redis",
    "connection",
    "pool"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/urbanhire/redis-pool-connection.git"
  },
  "author": "Urbanhire.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/urbanhire/redis-pool-connection/issues"
  },
  "homepage": "https://github.com/urbanhire/redis-pool-connection#readme",
  "dependencies": {
    "lodash.result": "^4.5.2",
    "redis": "^2.7.1",
    "redis-delete-wildcard": "0.0.3"
  },
  "standard": {
    "ignore": [
      "/test/*.js",
      "**/*.opts"
    ]
  },
  "pre-commit": [
    "test"
  ],
  "devDependencies": {
    "decache": "^4.1.0",
    "env2": "^2.1.1",
    "istanbul": "^0.4.5",
    "pre-commit": "^1.2.2",
    "standard": "^10.0.2",
    "tape": "^4.6.3"
  }
}
